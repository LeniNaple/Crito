@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "LayoutWithShowcase.cshtml";
    ViewData["PageTitle"] = @Model.Value("pageTitle");
	ViewData["PageDescription"] = @Model.Value("pageDescription");
	ViewData["PageKeywords"] = @Model.Value("pageKeywords");
}

 
 
 @await Html.PartialAsync("_BrandSection")
 
 
 
 <section class="features ">
    <div class="features-container container">
        <div class="left-features">
            <p>@Model.Value("featureSectionTitle")</p>
            <h1>@Model.Value("featureSectionDescription")</h1>
            <a href="@Model.Value("featureSectionButtonUrl")" class="yellow-btn">@Model.Value("featureSectionButtonText")<i class="fa-solid fa-arrow-up-right"></i></a>
        </div>
        
           
             @Html.GetBlockListHtml(Model.ServiceFeatures)
             
        
    </div>
</section>