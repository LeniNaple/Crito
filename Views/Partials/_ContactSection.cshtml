@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage


<section class="contact-section container">
    <div class="contact-section-box">
        <button><i class="fa-sharp fa-solid fa-location-dot" style="color: #ffffff;"></i></button>
        <div>
            <h3>Visit Us</h3>
            <p>@Model.Value("contactInformationStreetName")</p>
            <p>@Model.Value("contactInformationPostalCode") @Model.Value("contactInformationCity")</p>
        </div>
    </div>

    <div class="contact-section-box">
        <button><i class="fa-solid fa-phone" style="color: #ffffff;"></i></button>
        <div>
            <h3>Call Us</h3>
            <p>@Model.Value("contactInformationPhoneNumber1")</p>
            <p>@Model.Value("contactInformationPhoneNumber2")</p>
        </div>
        
    </div>

    <div class="contact-section-box">
        <button><i class="fa-solid fa-envelope" style="color: #ffffff;"></i></button>
        <div>
            <h3>Email Us</h3>
            <p>@Model.Value("contactInformationEmail1")</p>
            <p>@Model.Value("contactInformationEmail2")</p>
        </div>
    </div>
</section>