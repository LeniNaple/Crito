
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockListItem>;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
    var content = (ContentModels.OurServicesOptions)Model.Content;
}


<div class="service-card">
    <div class="gray-line"></div>
    <h2>@content.Value("ourServicesOptionsTitle")</h2>
    <p>@content.Value("ourServicesOptionsDescription")</p>
    <a href="@content.Value("ourServicesOptionsUrl")" class="black-btn"><i class="fa-light fa-arrow-right"></i></a>
</div>